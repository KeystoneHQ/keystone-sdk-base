{
  "name": "@keystonehq/bc-ur-registry-ergo",
  "version": "0.1.0",
  "description": "bc-ur-registry extension for Ergo",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.mts"
    }
  },
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "pnpm ts-bridge --project tsconfig.build.json --verbose --clean",
    "test": "jest --passWithNoTests"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@keystonehq/bc-ur-registry": "^0.6.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "@types/uuid": "^8.3.1",
    "typescript": "^5.2.2"
  }
}
