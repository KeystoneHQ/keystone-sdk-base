{
  "name": "@keystonehq/metamask-keystone-webusb-bridge",
  "version": "0.1.0-alpha.2",
  "description": "keystone webusb eth keyring for metamask",
  "author": "aaronisme <aarondongchen@gmail.com>",
  "homepage": "https://github.com/KeystoneHQ/keystone-airgaped-base#readme",
  "license": "MIT",
  "directories": {
    "lib": "dist"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist/",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeystoneHQ/keystone-airgaped-base.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "pnpm ts-bridge --project tsconfig.build.json --verbose --clean"
  },
  "dependencies": {
    "@keystonehq/metamask-keystone-usb-keyring": "^0.1.0-alpha.4",
    "@keystonehq/hw-transport-webusb": "0.5.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "typescript": "^5.2.2"
  },
  "bugs": {
    "url": "https://github.com/KeystoneHQ/keystone-airgaped-base/issues"
  },
  "gitHead": "83d8e223d29e5cc71dccc963388d65a87c894636"
}
