{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "lib": ["dom", "esnext"],
        "importHelpers": true,
        "declaration": true,
        "sourceMap": true,
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "noImplicitAny": false,
        "strictNullChecks": false,
        "strictFunctionTypes": true,
        "noImplicitThis": false,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "jsx": "react",
    },
}
